<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.snooper.app.controller.TitleBarController">
   <children>
      <AnchorPane prefHeight="44.0" prefWidth="600.0">
