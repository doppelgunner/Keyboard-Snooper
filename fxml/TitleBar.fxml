            <HBox fx:id="titleBarHBox" alignment="CENTER_RIGHT" layoutX="1.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="44.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
